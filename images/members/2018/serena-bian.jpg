  <form id="hiddenget" action="https://www.google.com/accounts/AccountChooser?hd=sas.upenn.edu&amp;continue=https%3A%2F%2Fdrive.google.com%2Fa%2Fsas.upenn.edu%2Ffile%2Fd%2F0BxTk1pr7UxOcTlYzMFhIcURidkE%2Fview%3Fusp%3Dsharing&amp;followup=https%3A%2F%2Fdrive.google.com%2Fa%2Fsas.upenn.edu%2Ffile%2Fd%2F0BxTk1pr7UxOcTlYzMFhIcURidkE%2Fview%3Fusp%3Dsharing&amp;service=wise" method="get">
  <noscript>
  You should turn on Javascript support.
  <input type="submit" id="nojssubmit" value="Continue">
  </noscript>
</form>
  <script>
window.onload = function() {
  var redirectUrl = 'https:\x2F\x2Fwww.google.com\x2Faccounts\x2FAccountChooser?hd=sas.upenn.edu\x26continue=https%3A%2F%2Fdrive.google.com%2Fa%2Fsas.upenn.edu%2Ffile%2Fd%2F0BxTk1pr7UxOcTlYzMFhIcURidkE%2Fview%3Fusp%3Dsharing\x26followup=https%3A%2F%2Fdrive.google.com%2Fa%2Fsas.upenn.edu%2Ffile%2Fd%2F0BxTk1pr7UxOcTlYzMFhIcURidkE%2Fview%3Fusp%3Dsharing\x26service=wise';
  var domain = 'sas.upenn.edu';
  var hash = window.location.hash;
  if (hash) {
  var match = hash.match(/[#&]Email=([^&]+)/);
  if (match) {
  redirectUrl += "&Email=" + match[1] + "@" + domain;
  }
  }
  window.location.replace(redirectUrl);
};
</script>
